<configuration>
  <system.webServer>
    <staticContent>
      <clientCache cacheControlMode="UseMaxAge" cacheControlMaxAge="365.00:00:00" />
    </staticContent>
    <httpCompression 
          noCompressionForHttp10="false"
          noCompressionForProxies="false"
          sendCacheHeaders="true"
          minFileSizeForComp="0" />
  </system.webServer>
</configuration>
