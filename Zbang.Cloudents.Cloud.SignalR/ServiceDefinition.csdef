<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="Zbang.Cloudents.Cloud.SignalR" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2014-01.2.3">
  <WebRole name="Zbang.Cloudents.WebAppSignalR" vmsize="Small">
    <Sites>
      <Site name="Web">
        <Bindings>
          <Binding name="Endpoint1" endpointName="Endpoint1" />
          <Binding name="Endpoint2" endpointName="Endpoint2" />
        </Bindings>
      </Site>
    </Sites>
    <Endpoints>
      <InputEndpoint name="Endpoint1" protocol="http" port="80" />
      <InputEndpoint name="Endpoint2" protocol="https" port="443" certificate="CloudDents" />
    </Endpoints>
    <Imports>
      <Import moduleName="Diagnostics" />
      <Import moduleName="RemoteAccess" />
      <Import moduleName="RemoteForwarder" />
    </Imports>
    <Certificates>
      <Certificate name="CloudDents" storeLocation="LocalMachine" storeName="My" />
      <Certificate name="MyIntermediateCertCloudDents" storeLocation="LocalMachine" storeName="CA" />
      <Certificate name="MyRootCertCloudDents" storeLocation="LocalMachine" storeName="Trust" />
    </Certificates>
    <ConfigurationSettings>
      <Setting name="WebSiteUrl" />
    </ConfigurationSettings>
  </WebRole>
</ServiceDefinition>