<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
    
    
  </configSections>
  <dataCacheClients>
    <dataCacheClient>
      <!--To use the in-role flavor of Windows Azure Caching, set identifier to be the cache cluster role name -->
      <!--To use the Windows Azure Caching Service, set identifier to be the endpoint of the cache cluster -->
      <autoDiscover isEnabled="true" identifier="zboxcache.cache.windows.net" />
      
      
      
    </dataCacheClient>
  </dataCacheClients>
  <log4net>
    <appender name="NHibernateLogFile" type="log4net.Appender.FileAppender">
      <file value="C:/temp/Logs/NHibernate2.log" />
      <appendToFile value="true" />
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date{HH:mm:ss.fff}%m%n==========%n" />
      </layout>
    </appender>
    <logger name="NHibernate" additivity="false">
      <level value="WARN" />
      <appender-ref ref="NHibernateLogFile" />
    </logger>
    <logger name="NHibernate.SQL" additivity="false">
      <level value="DEBUG" />
      <appender-ref ref="NHibernateLogFile" />
    </logger>
  </log4net>
  <connectionStrings>
    <add name="ZBox" connectionString="Data Source=(local);Initial Catalog=ZBox;Integrated Security=SSPI;" providerName="System.Data.SqlClient" />
    <add name="Hatavot" connectionString="Data Source=mssql4.evhost.co.il;Initial Catalog=bizpoin_mainDB;Persist Security Info=True;User ID=bizpoin_bizpointDB;Password=Plus2013Dot2" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <appSettings>
    <add key="DataCache" value="AzureMVC1" />
  </appSettings>
