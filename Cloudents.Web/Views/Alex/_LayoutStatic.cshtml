@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="@System.Threading.Thread.CurrentThread.CurrentUICulture.TwoLetterISOLanguageName">
<head>
    @*@Html.Css2("staticPage")*@
    <link href="~/alex/home.css" asp-append-version="true" rel="stylesheet" />
    <meta name="viewport" content="width=device-width, initial-scale=1, minimal-ui,user-scalable=yes">
</head>
<body class="internal-page @ViewBag.page-page">
    <div class="page-overlay-wrapper">
        <div class="fade-gradient_NOPE"></div>
    </div>
    @{Html.RenderPartial("_StaticPageHeader");}
    <div class="content">
        <img class="coffee" src="~/alex/coffee2_500px.png" />
        @RenderBody()
    </div>
    @* ReSharper disable once Mvc.PartialViewNotResolved *@
    <div id="cssCheck"></div>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.4/jquery.min.js"></script>
    <script src="~/alex/helpers.js"></script>
    <script src="~/alex/normal.js"></script>
    @*@Html.Script2("alex")*@
