<div class="promo-box box-style" ng-hide="lb.hidePromo">
    <div class="top">
        <div class="title"></div>
        <flip-clock></flip-clock>
    </div>
    <div class="bottom">
        <div class="stats">
            <div class="rank">
                <div class="title">Your Rank</div>
                <div class="count">
                    <div class="num" ng-bind="::lb.flashcardPromo.row"></div>
                    <div class="icon"></div>
                </div>
            </div>
            <div class="likes">
                <div class="title">Your Likes</div>
                <div class="count">
                    <div class="num" ng-bind="::lb.flashcardPromo.count"></div>
                    <div class="icon"></div>
                </div>
            </div>
        </div>
        <div class="info" ng-click="lb.details()">click</div>
    </div>
</div>
