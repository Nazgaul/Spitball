@{ Html.RenderPartial("_Create");}
<div class="class">
    <div class="page-head">
        <div class="page-title">
            <h1>{{b.name}}</h1>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="portlet box">
                <div class="portlet-body">
                    <div class="tabbable tabbable-tabdrop">
                        <tabset>
                            <tab active="false" ui-sref-active="active" ui-sref="box.feed" heading="Feed"></tab>
                            <tab ui-sref-active="active" ui-sref="box.items" heading="Items"></tab>
                            <tab ui-sref-active="active" ui-sref="box.quiz" heading="Quizzes"></tab>
                            <tab ui-sref-active="active" ui-sref="box.members" heading="Members"></tab>
                        </tabset>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div ui-view></div>
    <button type="button" class="add" ng-click="b.uploadOn = true" ng-hide="b.uploadOn">+</button>
</div>

