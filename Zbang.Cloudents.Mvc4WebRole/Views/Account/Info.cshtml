<div class="tab-pane active" id="info">
    <div class="form-group">
        <div class="fileinput" data-provides="fileinput">
            <div class="thumbnail" style="width: 150px; height: 150px;">
                <img src="{{a.data.image}}" alt="" width="150" height="150" />
                <a plupload="i.fileUpload.url" plupload-options="i.fileUpload.options" class="bg-grey-cascade ontop" plupload-callbacks="i.fileUpload.callbacks">
                    Update Picture
                </a>
            </div>
           
        </div>
        <form role="form" ng-submit="i.submit()">
            <div class="form-group">
                <label class="control-label" for="firstName">First Name</label>
                <input type="text" id="firstName" placeholder="{{a.data.firstName}}" ng-model="a.firstName" class="form-control" />
            </div>
            <div class="form-group">
                <label class="control-label" for="lastName">Last Name</label>
                <input type="text" id="lastName" placeholder="{{a.data.lastName}}" ng-model="a.lastName" class="form-control" />
            </div>
            <div class="form-group">
                <label class="control-label" for="email">Change mail</label>
                <input type="email" id="email" placeholder="{{::a.data.email}}" class="form-control" />
            </div>
            <div class="form-group">
                <label class="control-label" for="univeristy">change university</label>
                @*<md-autocomplete md-input-id="university"
                                     md-no-cache="false"
                                     md-selected-item="a.selectedItem.name"
                                     md-search-text="a.searchText"
                                     md-selected-item-change="a.selectedItemChange(item)"
                                     md-items="item in a.querySearch(a.searchText)"
                                     md-item-text="item.name"
                                     md-min-length="2"
                                     md-delay="250"
                                     placeholder="{{::a.data.university}}">
                        <md-item-template>
                            <span md-highlight-text="ctrl.searchText" md-highlight-flags="^i">{{item.name}}</span>
                        </md-item-template>
                        <md-not-found>
                            No matches found for "{{a.searchText}}".
                        </md-not-found>
                    </md-autocomplete>*@
                <input type="text" ng-model="asyncSelected" placeholder="{{::a.data.university}}" class="form-control"
                       typeahead="item.name for item in i.querySearch($viewValue)"
                       typeahead-wait-ms="250"
                       typeahead-min-length="2"
                       typeahead-on-select="i.selectedItemChange($item)" />
            </div>

            <div class="margin-top-10 text-right">

                <button type="submit" class="btn blue">Save changes</button>
                <button type="reset" class="btn default">Cancel</button>
            </div>
        </form>
    </div>
</div>
