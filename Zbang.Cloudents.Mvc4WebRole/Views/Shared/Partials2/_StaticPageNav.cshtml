            <div><a href="@Url.RouteUrl("Privacy")"><span>@FooterResources.PrivacyPolicy</span></a></div>
            <div><a href="@Url.RouteUrl("TOS")"><span>@FooterResources.TermsOfService</span></a></div>
            <div><a href="@Url.RouteUrl("Advertise")"><span>@SharedResources.Advertise</span></a></div>
