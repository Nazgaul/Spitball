@using Zbang.Cloudents.Mvc4WebRole.Views.Account.Resources
@using Zbang.Cloudents.Mvc4WebRole.Views.Home.Resources
@using Zbang.Cloudents.Mvc4WebRole.Views.Shared.Resources
@using HomeResources = Zbang.Cloudents.Mvc4WebRole.Views.Account.Resources.HomeResources

<div class="top-image">
    <div class="text">@AboutUsResources.Subtitle</div>
</div>
<div class="nav-bar-wrapper">
    <div class="nav-bar">
        <button type="button" class="nav-prev">@Html.Svg("~/images/site/box-icons.svg", "arrow")</button>
        <div class="items-container">
            <div><a href="@Url.RouteUrl("AboutUs")"><span>@HomeResources.AboutCloudents</span></a></div>
