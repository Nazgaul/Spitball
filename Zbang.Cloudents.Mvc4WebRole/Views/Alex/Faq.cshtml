						@{Html.RenderPartial("_faq", new {J = 2, Question = "Why doesn't my class have files?", Answer = "Your class could be new, or none of your classmates have posted documents online. Be the first to upload! Share syllabi, old exams, study guides, and more. You'll be the class hero."});}
