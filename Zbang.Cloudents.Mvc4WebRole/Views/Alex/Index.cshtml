@{
	Layout = null;
}
<!doctype html>
<html class="no-js">
<head>
	@Html.Css2("alex")
	<meta charset="utf-8">
	<title>Alex</title>
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no"/>
</head>
<body class="home-page fader">

<div class="page-overlay-wrapper">
	<div class="fade-gradient_NOPE"></div>

</div>

<div class="header-wrapper">
	<header class="header fluid-width mobile-full-width">
		<a class="fader-link close-overlay" href="#">
			<h1 class="spitball-logo float-left">spitball</h1>
		</a>
		<div class="header-nav-links">
			<a class="header-nav-link hover-color about-link about-header-link" href="#">About</a>
			<a class="header-nav-link hover-color faq-link faq-header-link" href="#">FAQ</a>
		</div>
		<a class="menu" href="">Menu</a>
	</header>
</div>

<section class="fixed-content fluid-width">
	<div class="content">
		<div class="constrain">

			<div class="content-text">
				<div class="text-slide active" data-slide="0">
					<h2 class="js-animate-this">Class Materials</h2>
					<div class="js-animate-this">
						Tests, study guides, notes, homeworks and more!
					</div>
					<a href="https://itunes.apple.com/us/app/id990911114?utm_source=website&utm_medium=homepage">
						<img src="@Url.CdnContent("~/Images/Alex/appIcon.png")"/>
					</a>
				</div>
				<div class="text-slide" data-slide="1">
					<h2 class="js-animate-this">Textbooks</h2>
					<div class="js-animate-this">
						Rent, buy or sell textbooks at the best prices
					</div>

				</div>
				<div class="text-slide" data-slide="2">
					<h2 class="js-animate-this">Flashcards</h2>
					<div class="js-animate-this">
						Test yourself with our student-made flashcards
					</div>
				</div>
				<div class="text-slide" data-slide="3">
					<h2 class="js-animate-this">Tutors</h2>
					<div class="js-animate-this">
						24/7 help from in-person or online experts
					</div>
				</div>

			</div>
			<div class="device-frame">
				<div class="image-slides">
					<img class="image-slide init-image" alt="Docs - How Remarketing Keeps Customers Coming Back" src="@Url.CdnContent("~/Images/Alex/Docs_240px.png")"/>
					<img class="image-slide init-image" alt="Content - Get Customers Interested by Telling a Great Story" src="@Url.CdnContent("~/Images/Alex/unnamed_240px.png")"/>
					<img class="image-slide init-image" alt="Flashcard- How Remarketing Keeps Customers Coming Back" src="@Url.CdnContent("~/Images/Alex/FC_240px.png")"/>
					<img class="image-slide init-image" alt="Tutor - Advertising Content Measurement Strategy" src="@Url.CdnContent("~/Images/Alex/tutor_240px.png")"/>
				</div>
				<img class="init-image device" src="@Url.CdnContent("~/Images/Alex/iphone_300px.png")"/>
			</div>
		</div>
	</div>

</section>
<section class="scroll-container">
	<section class="scroll-section scroll-section-one" data-from-time="0.01" data-to-time="1.33" data-reverse-from-time="" data-reverse-to-time="">
