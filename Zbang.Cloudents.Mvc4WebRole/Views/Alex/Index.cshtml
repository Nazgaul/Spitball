@{
	Layout = null;
}
<!doctype html>
<html class="no-js">
<head>
	<meta charset="utf-8">
	<title>Alex</title>
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
	<link href="~/Content/Alex/home.css" rel="stylesheet">
	<link href="~/Content/Alex/jquery.fullPage.css" rel="stylesheet">

</head>
<body class="home-page fader">

<div class="page-overlay-wrapper">
<div class="fade-gradient_NOPE"></div>

</div>

<div class="header-wrapper">
	<header class="header fluid-width mobile-full-width">
		<a class="fader-link close-overlay" href="#" ><h1 class="primer-logo float-left">Primer</h1></a>
		<div class="header-nav-links">
			<a class="header-nav-link hover-color about-link about-header-link"  href="#">About</a>
			<a class="header-nav-link hover-color faq-link faq-header-link" href="#">FAQ</a>
		</div>
		<a class="menu" href="">Menu</a>
	</header>
</div>

<section class="fixed-content fluid-width">
	<div class="content">
		<div class="constrain">
          
			<div class="content-text">
				<div class="text-slide active" data-slide="0">
					<h2 class="js-animate-this">Class Materials</h2>
					<div class="js-animate-this">
						Tests, study guides, notes, homeworks and more!
					</div>
					<img src="~/Images/Alex/appIcon.png" />
				</div>
				<div class="text-slide" data-slide="1">
					<h2 class="js-animate-this">Textbooks</h2>
					<div class="js-animate-this">
						Rent, buy or sell textbooks at the best prices
					</div>
				
				</div>
				<div class="text-slide" data-slide="2">
					<h2 class="js-animate-this">Flashcards</h2>
					<div class="js-animate-this">
						Test yourself with our student-made flashcards
					</div>				
				</div>
				<div class="text-slide" data-slide="3">
					<h2 class="js-animate-this">Tutors</h2>
					<div class="js-animate-this">
						24/7 help from in-person or online experts
					</div>
				</div>
				
			</div>
			<div class="device-frame">
				<div class="image-slides">
					<img class="image-slide init-image" alt="Advertising - How Remarketing Keeps Customers Coming Back" src="~/Images/Alex/Docs.png"/>
					<img class="image-slide init-image" alt="Content - Get Customers Interested by Telling a Great Story" src="~/Images/Alex/unnamed.png"/>
					<img class="image-slide init-image" alt="Lesson Recap - How Remarketing Keeps Customers Coming Back" src="~/Images/Alex/FC.png"/>
					<img class="image-slide init-image" alt="Lesson Categories - Advertising Content Measurement Strategy" src="~/Images/Alex/tutor.png"/>
				</div>
				<img class="init-image device" src="~/Images/Alex/iphone.png"/>
			</div>
		</div>
	</div>

</section>
<section class="scroll-container">
	<section class="scroll-section scroll-section-one" data-from-time="0.01" data-to-time="1.33" data-reverse-from-time="" data-reverse-to-time="">
		<div class="scroll-section-content">
			<img class="layer-object apple initial-state init-image" src="~/Images/Alex/apple.png" />
			<img class="layer-object headphones initial-state init-image " src="~/Images/Alex/headphone.png" />
			<img class="layer-object spinner initial-state init-image" src="~/Images/Alex/spinner_small.png" />
		</div>
        
	</section>
	<section class="scroll-section scroll-section-two" data-from-time="1.50" data-to-time="2.67" data-reverse-from-time="6.73" data-reverse-to-time="8.07">
		<div class="scroll-section-content">
			<img class="layer-object book js-load-async" src="~/Images/Alex/book_small.png" />
			<img class="layer-object coffee js-load-async" src="~/Images/Alex/coffee2small.png" />
			<img class="layer-object sp-note js-load-async" src="~/Images/Alex/sp_notebook.png" />
		</div>
        
	</section>
	<section class="scroll-section scroll-section-three" data-from-time="2.83" data-to-time="4.00" data-reverse-from-time="5.40" data-reverse-to-time="6.57">
		<div class="scroll-section-content">
			<img class="layer-object folder js-load-async" src="~/Images/Alex/red_folder.png" />
			<img class="layer-object sun-glasses js-load-async" src="~/Images/Alex/sun_glasses_small.png" />
			<img class="layer-object pizza js-load-async" src="~/Images/Alex/pizza_small.png" />
		</div>
        
	</section>
	<section class="scroll-section scroll-section-four" data-from-time="" data-to-time="" data-reverse-from-time="4.06" data-reverse-to-time="5.23">
		<div class="scroll-section-content">
			<img class="layer-object doc js-load-async" src="~/Images/Alex/doc_small.png" />
			<img class="layer-object coffee js-load-async" src="~/Images/Alex/coffee2small.png" />
			<img class="layer-object mac js-load-async" src="~/Images/Alex/mac_small.png" />
			<img class="layer-object calc js-load-async" src="~/Images/Alex/calculator.png" />
		</div>
	</section>
</section>

