			<picture>
				<source class="layer-object apple js-load-async" srcset="@Url.CdnContent("~/Images/Alex/apple_360px.png")" media="(min-width: 769px)">
				<img class="layer-object apple js-load-async" src="@Url.CdnContent("~/Images/Alex/appIcon.png")"/>
			</picture>
			<picture>
