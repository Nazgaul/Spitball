<configuration>
  <system.webServer>
    <staticContent>
      <clientCache cacheControlCustom="public" cacheControlMode="UseMaxAge" cacheControlMaxAge="30.00:00:00" />
    </staticContent>
    <httpCompression >
      <staticTypes>
        <add mimeType="image/*" enabled="true"/>
      </staticTypes>
    </httpCompression>
  </system.webServer>
</configuration>
