<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Zbang.Zbox.Domain"
                   namespace="Zbang.Zbox.Domain">
  <class name="Product">
    <id name="Id" column="Id">
      <generator class="assigned"/>
    </id>
    <property name="Name"  />
    <property name="ExtraDetails"  />
    <property name="NumberOfSales"  />
    <property name="Coupon"  />
    <property name="SalePrice"  />
    <property name="PictureUrl"  />
  </class>
</hibernate-mapping>
