<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Zbang.Zbox.Domain"
                   namespace="Zbang.Zbox.Domain">
  <class name="RussianDepartment" table="Department" >
    <id name="Id" column="Id" type="long">
      <generator class="native"></generator>
    </id>
    <many-to-one column="UniversityId2" class="University" name="University" foreign-key="UniversityUser2"/>

    <property name="Name" />
    <property name="Year" />
  </class>

  <class name="Student" table="Student" >
    <id name="StudentId" column="StudentId" type="long">
      <generator class="native"></generator>
    </id>
    <many-to-one column="UniversityId2" class="University" name="University" foreign-key="UniversityStudent2"/>
    <property name="ID" />
  </class>

