<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="Zbang.Cloudents.Cloud.BackgroundWorker2" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2015-04.2.6">
  <WorkerRole name="Zbang.Zbox.WorkerRoleSearch" vmsize="Small">
    <Imports>
      <Import moduleName="RemoteAccess" />
      <Import moduleName="RemoteForwarder" />
    </Imports>
    <ConfigurationSettings>
      <Setting name="StorageConnectionString" />
      <Setting name="StorageCdnEndpoint" />
      <Setting name="ZBox" />
      <Setting name="AzureSearchKey" />
      <Setting name="AzureSeachServiceName" />
      <Setting name="Microsoft.WindowsAzure.Plugins.Diagnostics.ConnectionString" />
      <Setting name="ServiceBusConnectionString" />
      <Setting name="ServiceBusHubName" />
      <Setting name="platform" />
      <Setting name="signalR" />
      <Setting name="NumberOfEmailsPerHour" />
      <Setting name="NumberOfSpamGunEmailBatch" />
      <Setting name="SearchProduction" />
      <Setting name="APPINSIGHTS_INSTRUMENTATIONKEY" />
      <Setting name="documentDbProduction" />
      <Setting name="CacheUse" />
      <Setting name="TimeToExtractText" />
    </ConfigurationSettings>
    <LocalResources>
      <LocalStorage name="ItemPreviewStorage" cleanOnRoleRecycle="false" sizeInMB="222000" />
    </LocalResources>
  </WorkerRole>
</ServiceDefinition>